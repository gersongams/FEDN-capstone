{
  "name": "capstone-travel-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "test": "jest",
    "start": "npm run client-prod && npm run server-prod",
    "dev": "concurrently --kill-others \"yarn client-dev\" \"yarn server-dev\"",
    "server-dev": "nodemon src/server/server.js",
    "server-prod": "node src/server/server.js",
    "client-prod": "webpack --config webpack.prod.js",
    "heroku-prebuild": "yarn install --production=false",
    "client-dev": "webpack-dev-server --host 0.0.0.0 --port 7000 --config webpack.dev.js --open"
  },
  "keywords": [],
  "author": "@gersongams",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
